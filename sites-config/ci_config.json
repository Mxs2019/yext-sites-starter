{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist/"
      }
    ],
    "features": "sites-config/features.json",
    "plugins": [
      {
        "pluginName": "Generator",
        "sourceFiles": [
          {
            "root": "./node_modules/@yext/yext-sites-scripts/node_modules/ssg-plugin/plugin",
            "pattern": "**/*.{ts,json}"
          },
          {
            "root": ".yext/",
            "pattern": "manifest.json"
          },
          {
            "pattern": "dist/**/*.js"
          }
        ],
        "event": "ON_PAGE_GENERATE",
        "functionName": "Generate"
      }
    ]
  },
  "dependencies": {
    "installDepsCmd": "npm install",
    "requiredFiles": [
      "package.json",
      "package-lock.json",
      ".node-version"
    ]
  },
  "buildArtifacts": {
    "buildCmd": "rm -rf .artifact_output/ && npm run build"
  },
  "livePreview": {
    "serveSetupCmd": ":"
  }
}